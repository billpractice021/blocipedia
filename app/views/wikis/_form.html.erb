<%= form_for @wiki do |w| %>
  <div class="form-group">
  <%= w.label :title %>
  <%= w.text_field :title %>
  </div>
  <div class="form-group">
  <%= w.label :body %>
  <%= w.text_area :body %>
  </div>
  <% if current_user.premium? || current_user.admin? %>
    <div class="form-group">
      <%= w.check_box :private %>
      <%= w.label :private %>
    </div>
  <% end %>
  <%= w.submit "save" %>
<% end %>